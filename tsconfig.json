{
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2022",
		"declaration": true,
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
